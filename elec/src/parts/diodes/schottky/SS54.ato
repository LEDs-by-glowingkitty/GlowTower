component SS54:
    """
    This is a schottky diode, rated for 40V and 5A.
    It has a voltage drop of 550mV at 5A.
    Datasheet: https://datasheet.lcsc.com/lcsc/2303141100_MDD-Microdiode-Electronics--SS54_C22452.pdf
    """
    # component SS54
    footprint = "SMA_L4.4-W2.8-LS5.4-R-RD"
    lcsc_id = "C22452"
    mpn = "C22452"
    # pins
    signal A ~ pin 1
    signal K ~ pin 2

    signal input ~ A
    signal output ~ K